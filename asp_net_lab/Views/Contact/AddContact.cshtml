@{
}
<form asp-action="AddContact" method="post">
    <div>
        <label asp-for="Name">Wprowadź imię</label><br />
        <input class="form-control" asp-for="Name" />
        <div>
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
    </div>
     <div>
        <label asp-for="Age">Wprowadź wiek</label><br />
        <input class="form-control" asp-for="Age" />
        <div>
            <span asp-validation-for="Age" class="text-danger"></span>
        </div>
    </div>
     <div>
        <label asp-for="Email">Wprowadź email</label><br />
        <input class="form-control" asp-for="Email" />
        <div>
            <span asp-validation-for="Email" class="text-danger"></span>
        </div>
    </div>
    <div>
        <button class="btn btn-primary mt-3"> Zatwierdź</button>
    </div>
    
</form>
